{
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "/bin/bash"
        }
    },
    "lldb.executable": "/usr/bin/lldb",
    // VS Code don't watch files under ./target
    "files.watcherExclude": {
        "**/target/**": true
    },
    "files.exclude": {
        "**/.devcontainer": true
    }
}